function MagX(MagDis)
    if MagZ then
        return MagZ(MagDis)
    else
        return (MagDis.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 200
    end
end
function BringMob(pos)
    QuestCheck()
    for _, v in ipairs(workspace.Playability.Enemys[IslandMon]:GetChildren()) do
        if string.find(v.Name, NameMon) and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position - pos.Position).Magnitude <= 350 then
            if v:FindFirstChildOfClass("Humanoid") then
                local humanoid = v.Humanoid
                if humanoid:FindFirstChild("Animator") then
                    humanoid.Animator:Destroy()
                end

                if MagX(v.HumanoidRootPart) then
                    v.HumanoidRootPart.CFrame = pos
                    humanoid.JumpPower = 0
                    humanoid.WalkSpeed = 0
                    humanoid.Sit = true    
                    humanoid.PlatformStand = true
                    humanoid:ChangeState(11)
                    for _, part in ipairs(v:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                    sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius",math.huge)
                end
            end
        end
    end
end

