do
    function TweenFarm(gotoCFrame)
        pcall(function()
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            end)
                if (game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude <= 30 then
                    pcall(function() 
                        tweenz:Cancel()
                    end)
                    game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.CFrame = gotoCFrame
                else
                local tween_s = game:service"TweenService"
                local info = TweenInfo.new((game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude/280, Enum.EasingStyle.Linear)
                tween, err = pcall(function()
                tweenz = tween_s:Create(game.Players.LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = gotoCFrame})
                tweenz:Play()
            end)
        if not tween then return err end
        end
        function TweenCanCle()
            tweenz:Cancel()
        end
    end -- tweenfarm
    function TweenToLocation(gotoCFrame)
        pcall(function()
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            end)
                if (game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude <= 30 then
                    pcall(function() 
                        tweenlocate:Cancel()
                    end)
                    game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.CFrame = gotoCFrame
                else
                local tween_s = game:service"TweenService"
                local info = TweenInfo.new((game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - gotoCFrame.Position).Magnitude/280, Enum.EasingStyle.Linear)
                tween, err = pcall(function()
                tweenlocate = tween_s:Create(game.Players.LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = gotoCFrame})
                tweenlocate:Play()
            end)
        if not tween then return err end
        end
        function TweenCanCleLocation()
            tweenlocate:Cancel()
        end
    end -- tween
    function Click()
        game:GetService'VirtualUser':CaptureController()
    	game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
    end
    function ByPass(Position)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Position
        wait(1.75)
        game.Players.LocalPlayer.Character.Head:Destroy()
    end -- bypass
    function EquipWeapon(ToolSe)
        if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            Tool = game.Players.LocalPlayer.Backpack:WaitForChild(ToolSe)
            wait()
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(Tool)
            wait(0.10)
        end
    end
end -- function

